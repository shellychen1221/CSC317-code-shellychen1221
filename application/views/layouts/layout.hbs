<!DOCTYPE html>
<html>
  <head>
    <title>{{title}}</title>
    <link rel="icon" href="/public/favicon.ico" sizes="70x70" type="image/ico">
    <link rel='stylesheet' href='/public/css/style.css' />
    {{#each js}}
        <script defer src="public/js/{{this}}"> </script>
    {{/each}}

    {{!-- {{#if value}}

    {{/if}}

    {{#unless value}}

    {{/unless}}

    {{#if value}}
      this.data.user.name`
    {{/else}}

    {{/else}}

    {{#with this.data.user}}
        {{name}}
    {{/with}} --}}


  </head>
  
  <body>
    {{#if (nonEmptyObject messages)}}
      {{> flashmessage this}}
    {{/if}}
    <div id="container">
    {{>header}}
    {{>nav}}
    {{{body}}}
    {{>footer}}
    </div>
  </body>
</html>
